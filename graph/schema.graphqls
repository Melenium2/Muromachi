scalar Time
scalar FormattedDate

type Categories {
    id: Int!
    bundleId: Int!
    type: String!
    place: Int!
    date: Time!
}

type Keywords {
    id: Int!
    bundleId: Int!
    type: String!
    place: Int!
    date: Time!
}

type DeveloperContacts {
    email: String!
    contacts: String!
}

type Meta{
    id: Int!
    bundleId: Int!
    title: String!
    price: String!
    picture: String!
    screenshots: [String!]!
    rating: String!
    reviewCount: String!
    ratingHistogram: [String!]!
    description: String!
    shortDescription: String!
    recentChanges: String!
    releaseDate: String!
    lastUpdateDate: String!
    appsize: String!
    installs: String!
    version: String!
    osVersion: String!
    contentRating: String!
    devContacts: DeveloperContacts!
    privacyPolicy: String!
    date: Time!
}

type App{
    id: Int!
    bundle: String!
    category: String!
    developerId: String!
    developer: String!
    geo: String!
    startAt: Time!
    period: Int!
}

type Query {
    meta(id: Int!, last: Int, start: FormattedDate, end: FormattedDate): [Meta]!
    cats(id: Int!, last: Int, start: FormattedDate, end: FormattedDate): [Categories]!
    keys(id: Int!, last: Int, start: FormattedDate, end: FormattedDate): [Keywords]!
}
